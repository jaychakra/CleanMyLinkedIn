{
  "manifest_version": 2,
  "name": "Clean my LinkedIn",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": [
        "https://www.linkedin.com/*", "https://linkedin.com/*"
      ],
      "js": ["jquery.slim.min.js", "content.js"],
      "run_at": "document_idle"
    }
  ],
  "description": "Extension to remove the clutter from LinkedIn",
  "icons": {
    "16": "assets/CML16.png",
    "24": "assets/CML24.png",
    "32": "assets/CML32.png",
    "48": "assets/CML48.png",
    "64": "assets/CML64.png",
    "72": "assets/CML72.png",
    "96": "assets/CML96.png",
    "128": "assets/CML128.png",
    "256": "assets/CML256.png"
  }
}
